{
  "name": "@lsst/pik",
  "version": "0.7.0",
  "description": "Extensible CLI toolkit for simplifying developers' everyday tasks like config switching and worktree management",
  "type": "module",
  "license": "MIT",
  "author": "Yurii Tatar <lsst25@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lsst25/pik.git",
    "directory": "packages/cli"
  },
  "keywords": [
    "toolkit",
    "cli",
    "plugin",
    "config",
    "configuration",
    "switch",
    "toggle",
    "environment",
    "env",
    "worktree",
    "git",
    "developer-tools",
    "monorepo"
  ],
  "bin": {
    "pik": "./dist/bin/pik.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@pik/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "@inquirer/prompts": "^8.1.0",
    "@lsst/pik-core": "*",
    "@lsst/pik-plugin-select": "*",
    "@lsst/pik-plugin-worktree": "*",
    "commander": "^14.0.2",
    "picocolors": "^1.1.1"
  }
}
